    <Requires>
        Requires a separate Plex instance.
    </Requires>
    <Config Name="Web UI Port" Target="3000" Default="3000" Mode="tcp" Description="Container Port: 3000" Type="Port" Display="always-hide" Required="true" Mask="false">3000</Config>
    <Config Name="Plex Server URL" Target="PLEX_SERVER_URL" Default="" Description="URL of your Plex Server (e.g., http://localhost:32400)" Type="Variable" Display="always-hide" Required="true" Mask="false"/>
    <Config Name="Plex Token" Target="PLEX_TOKEN" Default="" Description="Plex token" Type="Variable" Display="always-hide" Required="true" Mask="true"/>
    <Config Name="Config Data" Target="/config" Default="/mnt/user/appdata/obzorarr/config" Description="Path to the config data" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/obzorarr/config</Config>
    <Config Name="PUID" Target="PUID" Default="1000" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">1000</Config>
