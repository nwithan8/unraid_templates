<?xml version="1.0"?>
<Container version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://raw.githubusercontent.com/nwithan8/unraid_templates/main/templates/template_schema.xsd template_schema.xsd">
    <Name>Lab Dash</Name>
    <Repository>ghcr.io/anthonygress/lab-dash:latest</Repository>
    <Registry>ghcr.io/anthonygress/lab-dash</Registry>
    <Branch>
        <Tag>latest</Tag>
        <TagDescription>Latest stable release</TagDescription>
    </Branch>
    <Network>bridge</Network>
    <WebUI>http://[IP]:[PORT:2022]/</WebUI>
    <Privileged>false</Privileged>
    <Support>https://github.com/AnthonyGress/lab-dash/issues</Support>
    <Project>https://github.com/AnthonyGress/lab-dash</Project>
    <Overview>
        An open-source user interface designed to manage your server/homelab applications and services.
    </Overview>
    <Beta>False</Beta>
    <Category>Productivity: Tools: Other: Status:Stable</Category>
    <ExtraSearchTerms>dashboard homelab server management labdash lab-dash</ExtraSearchTerms>
    <Icon>https://raw.githubusercontent.com/AnthonyGress/lab-dash/refs/heads/main/frontend/src/assets/gradient_logo.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/nwithan8/unraid_templates/main/templates/lab_dash.xml</TemplateURL>
    <Maintainer>
        <WebPage>https://github.com/nwithan8</WebPage>
    </Maintainer>
    <Changes>
        ### 2026-01-12

        Initial release
    </Changes>
    <Requires>optional: use privileged: true and network_mode: host for monitoring network usage stats.</Requires>
    <Config Name="Web UI Port" Target="2022" Default="2022" Mode="tcp" Description="Container Port: 2022" Type="Port" Display="always-hide" Required="true" Mask="false">2022</Config>
    <Config Name="Secret Key" Target="SECRET" Default="" Description="Generate with 'openssl rand -base64 32'" Type="Variable" Display="always-hide" Required="true" Mask="true"/>
    <Config Name="Config Path" Target="/config" Default="/mnt/user/appdata/lab-dash/config" Description="Path to the config folder" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/lab-dash/config</Config>
    <Config Name="Uploads Path" Target="/app/public/uploads" Default="/mnt/user/appdata/lab-dash/uploads" Description="Path to the uploads folder" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/lab-dash/uploads</Config>
    <Config Name="System Path" Target="/sys" Default="/sys" Description="Path to the system folder" Type="Path" Mode="ro" Display="advanced-hide" Required="true" Mask="false">/sys</Config>
    <Config Name="Docker Socket" Target="/var/run/docker.sock" Default="/var/run/docker.sock" Mode="ro" Description="Path to the Docker socket" Type="Path" Display="advanced-hide" Required="true" Mask="false">/var/run/docker.sock</Config>
    <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="User ID (Unraid: 99 = nobody)" Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
    <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Group ID (Unraid: 100 = users)" Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>
