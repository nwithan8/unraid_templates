<?xml version="1.0"?>
<Container version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://raw.githubusercontent.com/nwithan8/unraid_templates/main/template_schema.xsd template_schema.xsd">
    <Name>imgproxy</Name>
    <Repository>darthsim/imgproxy:latest</Repository>
    <Registry>https://hub.docker.com/r/darthsim/imgproxy</Registry>
    <Network>bridge</Network>
    <WebUI>http://[IP]:[PORT:8080]/</WebUI>
    <Privileged>false</Privileged>
    <Support>https://forums.unraid.net/topic/133764-support-grtgbln-docker-templates</Support>
    <Project>https://docs.imgproxy.net</Project>
    <Overview>
        A fast and secure standalone server for resizing, processing, and converting images. &#xD;
        Additional configuration available via environment variables: https://docs.imgproxy.net/configuration/options
    </Overview>
    <Beta>False</Beta>
    <Category>Tools: Productivity: Other: Status:Stable</Category>
    <Icon>https://raw.githubusercontent.com/nwithan8/unraid_templates/master/images/imgproxy-icon.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/nwithan8/unraid_templates/main/templates/imgproxy.xml</TemplateURL>
    <Maintainer>
        <WebPage>https://github.com/nwithan8</WebPage>
    </Maintainer>
    <Changes>
        ### 2024-06-06

        Initial release
    </Changes>
    <Config Name="WebUI" Target="8080" Default="8080" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="true" Mask="false">8080</Config>
</Container>
