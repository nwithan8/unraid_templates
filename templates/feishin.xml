<?xml version="1.0"?>
<Container version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://raw.githubusercontent.com/nwithan8/unraid_templates/main/templates/template_schema.xsd template_schema.xsd">
    <Name>Feishin</Name>
    <Repository>ghcr.io/jeffvli/feishin:latest</Repository>
    <Registry>ghcr.io/jeffvli/feishin</Registry>
    <Branch>
        <Tag>latest</Tag>
        <TagDescription>Latest stable release</TagDescription>
    </Branch>
    <Network>bridge</Network>
    <WebUI>http://[IP]:[PORT:9180]/</WebUI>
    <Privileged>false</Privileged>
    <Support>https://github.com/jeffvli/feishin/issues</Support>
    <Project>https://feishin.vercel.app/</Project>
    <Overview>
        A modern self-hosted music player.
    </Overview>
    <Beta>False</Beta>
    <Category>MediaApp:Video MediaServer:Video Productivity: Tools: Other: Status:Stable</Category>
    <ExtraSearchTerms>jellyfin music subsonic navidrome feishin audio player streamer spotify</ExtraSearchTerms>
    <Icon>https://raw.githubusercontent.com/jeffvli/feishin/development/assets/icons/icon.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/nwithan8/unraid_templates/main/templates/feishin.xml</TemplateURL>
    <Screenshot>https://raw.githubusercontent.com/jeffvli/feishin/development/media/preview_full_screen_player.png</Screenshot>
    <Screenshot>https://raw.githubusercontent.com/jeffvli/feishin/development/media/preview_album_artist_detail.png</Screenshot>
    <Screenshot>https://raw.githubusercontent.com/jeffvli/feishin/development/media/preview_album_detail.png</Screenshot>
    <Screenshot>https://raw.githubusercontent.com/jeffvli/feishin/development/media/preview_smart_playlist.png</Screenshot>
    <Maintainer>
        <WebPage>https://github.com/nwithan8</WebPage>
    </Maintainer>
    <Changes>
        ### 2025-11-17

        Initial release
    </Changes>
    <Config Name="Web UI Port" Target="9180" Default="9180" Mode="tcp" Description="Container Port: 9180" Type="Port" Display="always-hide" Required="true" Mask="false">9180</Config>
    <Config Name="Server Name" Target="SERVER_NAME" Default="" Description="Name of your music server" Type="Variable" Display="always-hide" Required="false" Mask="false"/>
    <Config Name="Server Type" Target="SERVER_TYPE" Default="jellyfin|navidrome" Description="Type of music server" Type="Variable" Display="always-hide" Required="true" Mask="false"/>
    <Config Name="Server URL" Target="SERVER_URL" Default="" Description="URL of your music server (e.g., http://localhost:8096)" Type="Variable" Display="always-hide" Required="true" Mask="false"/>
    <Config Name="Lock Settings" Target="SERVER_LOCK" Default="true|false" Description="When enabled, prevents changes to server settings via the web UI" Type="Variable" Display="always-hide" Required="true" Mask="false"/>
    <Config Name="PUID" Target="PUID" Default="099" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">099</Config>
    <Config Name="PGID" Target="PGID" Default="100" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">100</Config>
</Container>
