    <Requires>
        Expects a `config.toml` file to be mounted inside the Config Path before starting the container. See configuration details: https://maxdorninger.github.io/MediaManager/configuration.html &#xD;
        Requires a separate Postgres database. &#xD;
    </Requires>
    <Config Name="Web UI Port" Target="8000" Default="8000" Mode="tcp" Description="Container Port: 8000" Type="Port" Display="always-hide" Required="true" Mask="false">8000</Config>
    <Config Name="Data Path" Target="/data" Default="/mnt/user/appdata/media_manager/data" Description="Path to the data directory" Type="Path" Display="always-hide" Required="true" Mask="false">/mnt/user/appdata/media_manager/data</Config>
    <Config Name="Images Path" Target="/data/images" Default="/mnt/user/appdata/media_manager/data/images" Description="Path to the images directory" Type="Path" Display="always-hide" Required="true" Mask="false">/mnt/user/appdata/media_manager/data/images</Config>
    <Config Name="Config Path" Target="/app/config" Default="/mnt/user/appdata/media_manager/config" Description="Path to the config directory" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/media_manager/config</Config>
    <Config Name="Internal Config File Path" Target="CONFIG_DIR" Default="/app/config" Description="Internal path to the config directory. Not recommended to change." Type="Variable" Display="advanced-hide" Required="true" Mask="false">/app/config</Config>
</Container>
